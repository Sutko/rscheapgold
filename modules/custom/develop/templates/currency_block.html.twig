<div class="currency-menu">
    <ul class="nav nav-tabs">
        {% for item in currency %}
            <li {% if item.code == selected %} class="active" {% endif %}>
                <a data-sign="{{ item.sign }}" data-code="{{ item.code }}" href="javascript:void(0);">{{ item.name }}</a>
            </li>
        {% endfor %}
    </ul>
</div>